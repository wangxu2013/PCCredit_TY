<?xml version="1.0" encoding="UTF-8"?>
<validators>
	<validator id="examples.sample">
		<control name="seqNo">
			<entry type="number" value="true" />
		</control>
		<control name="name">
			<entry type="required" value="true" />
			<entry type="maxlength" value="50" />
		</control>
		<control name="type">
			<entry type="required" value="true" />
			<entry type="maxlength" value="50" />
		</control>
		<control name="nativeName">
			<entry type="required" value="true" />
			<entry type="maxlength" value="50" />
		</control>
		<control name="sampleDate">
			<entry type="required" value="true" />
			<entry type="datetime" value="yyyy-MM-dd" />
		</control>
		<control name="description">
			<entry type="maxlength" value="1000" />
		</control>
	</validator>
</validators>